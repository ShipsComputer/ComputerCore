<p id="notice"><%= notice %></p>

<h1>Sensor Arrays</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Vessel</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sensor_arrays.each do |sensor_array| %>
      <tr>
        <td><%= sensor_array.name %></td>
        <td><%= sensor_array.vessel %></td>
        <td><%= sensor_array.location %></td>
        <td><%= link_to 'Show', sensor_array %></td>
        <td><%= link_to 'Edit', edit_sensor_array_path(sensor_array) %></td>
        <td><%= link_to 'Destroy', sensor_array, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sensor Array', new_sensor_array_path %>
